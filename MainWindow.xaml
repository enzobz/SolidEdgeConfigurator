<Window x:Class="SolidEdgeConfigurator.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Main Window" Height="350" Width="525">
    <Grid>
        <Button Content="Open" Width="100" Height="30" Margin="10" VerticalAlignment="Top" />
        <Button Content="Add" Width="100" Height="30" Margin="10,50,0,0" VerticalAlignment="Top" />
        <Button Content="Remove" Width="100" Height="30" Margin="10,100,0,0" VerticalAlignment="Top" />
        <Button Content="Close" Width="100" Height="30" Margin="10,150,0,0" VerticalAlignment="Top" />
        <!-- Removed buttons related to Parts Management -->
    </Grid>
</Window>